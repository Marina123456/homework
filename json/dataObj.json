{
  "tag": "a",
  "content": "Пример ссылки",
  "attr":
  {
    "href": "#",
    "id": "someID"
  },
  "events":
  {
    "click": "alert (this.href);",
    "focus": "this.className='active'",
    "blur": "app.log(this)"
  },
  "style":
  {
    "width": "100px",
    "height": "67px",
    "color": "red"
  }
}